{
  "name": "RememberTabs",
  "description": "Remember Tab Group",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "tabs",
    "tabGroups",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/icon16.png",
      "32": "/images/icon32.png",
      "48": "/images/icon48.png",
      "128": "/images/icon.png"
    }
  }
}